{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "upload/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/users/:email",
      "methods": ["GET"],
      "dest": "dist/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://book-trekker-350ef.web.app",
        "Access-Control-Allow-Methods": "GET",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "dist/server.js"
    },
    {
      "src": "/(.+\\.[a-z]+)$",
      "dest": "/upload/$1"
    }
  ]
}
