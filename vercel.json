{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "upload/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "dist/server.js"
    },
    {
      "src": "/(.+\\.[a-z]+)$",
      "dest": "/upload/$1"
    }
  ]
}
